     "output_type": "execute_result"
    }
   ],
   "source": [
    "R = ['a','a','a','b','b','b','b','b']\n",
    "\n",
    "from collections import Counter\n",
    "from scipy import stats\n",
    "\n",
    "stats.entropy(list(Counter(R).values()), base=2)"
   ]
